script/source = "extends CharacterBody2D

@export_category(\"Variables\")
@export var walk_speed = 3.0
@export var initial_position = Vector2.ZERO
@export var input_direction = Vector2.ZERO
@export var is_moving = false
@export var percent_moved_to_next_tile = 0.0
@onready var anim_player = $AnimationPlayer
const TILE_SIZE = 16.0

func _ready(): #usada ao iniciar o jogo
	initial_position = position #atribuir a variavel à posição inicial do jogador

func _physics_process(delta): #essa função vai ser chamada a cada frame do jogo
	if is_moving == false:
		#player_view()
		player_input()
		move_and_slide()
	elif input_direction != Vector2.ZERO:
		move(delta)
	else:
		is_moving == false

#func player_view() -> void: player view
	#if input_direction.y == 0:
	

func player_input() -> void:
	if input_direction.y == 0: #verificando a direção (se nn é y, logo é x, ou seja para os lados)
		input_direction.x = int(Input.is_action_pressed(\"move_right\")) - int(Input.is_action_pressed(\"move_left\"))
	if input_direction.x == 0:
		input_direction.y = int(Input.is_action_pressed(\"move_down\")) - int(Input.is_action_pressed(\"move_up\"))
	if input_direction != Vector2.ZERO:
		initial_position = position
		is_moving = true

func move(delta):
	if Input.is_anything_pressed():
			if Input.is_action_pressed(\"move_down\"):
				anim_player.play(\"WalkDown\")
			elif Input.is_action_pressed(\"move_right\"):
				anim_player.play(\"WalkRight\")
			elif Input.is_action_pressed(\"move_left\"):
				anim_player.play(\"WalkLeft\")
			elif Input.is_action_pressed(\"move_up\"):
				anim_player.play(\"WalkUp\")
			if Input.is_action_just_released(\"move_down\"):
				anim_player.play(\"Down\")
			elif Input.is_action_just_released(\"move_right\"):
				anim_player.play(\"Right\")
			elif Input.is_action_just_released(\"move_left\"):
				anim_player.play(\"Left\")
			elif Input.is_action_just_released(\"move_up\"):
				anim_player.play(\"Up\")

	percent_moved_to_next_tile += walk_speed * delta
	if percent_moved_to_next_tile >= 1.0:
		position = initial_position + (TILE_SIZE * input_direction)
		percent_moved_to_next_tile = 0.0
		is_moving = false
	else:
		position = initial_position + (TILE_SIZE * input_direction * percent_moved_to_next_tile)
	
"

