window/size/viewport_height=360
window/size/mode=3
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/enabled=false

[dotnet]

project/assembly_name="cotiladventures"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/gui_auto_layout/plugin.cfg", "res://addons/quest_manager/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

